{
  "name": "radiai-backend",
  "version": "1.0.0",
  "description": "RadiAi MVP Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset"
  },
  "keywords": [
    "radiai",
    "news",
    "ai",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@supabase/supabase-js": "^2.97.0",
    "bullmq": "^5.70.0",
    "dotenv": "^16.4.1",
    "fastify": "^5.7.4",
    "ioredis": "^5.9.3",
    "openai": "^6.22.0",
    "pg": "^8.11.3",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "prisma": "^5.22.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
